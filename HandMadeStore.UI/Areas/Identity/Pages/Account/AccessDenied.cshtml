@page
@model AccessDeniedModel
@{
    ViewData["Title"] = "Access denied";
}

<header>
    <h1 class="text-danger text-center">@ViewData["Title"]</h1>
    <p class="text-danger text-center">You do not have access to this resource.</p>
    <div class="text-center" style="border:2px solid #593196; border-radius: 5px;">
        <span class="container-fluid py-3">  <img src="~/images/AccessDenied.png" height="20%" width="30%" /> </span>

    </div>

</header>
